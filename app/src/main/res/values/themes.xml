<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.LocationWake" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowActivityTransitions">true</item>



        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/fulvous</item>
        <item name="colorPrimaryVariant">@color/fulvous_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/space_cadet</item>
        <item name="colorSecondaryVariant">@color/space_cadet_light</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSecondary</item>
        <item name="android:windowContentTransitions">true</item>
    </style>
</resources>